{
  "name": "latex2html5",
  "version": "1.2.0",
  "description": "Author interactive math equations and diagrams online using LaTeX and PSTricks",
  "author": "pyramation@gmail.com",
  "main": "./build/index.js",
  "types": "./build/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/LaTeX2JS.git"
  },
  "keywords": [
    "latex",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "mathjax",
    "latex2html5"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rm -rf build && NODE_ENV=production babel --out-dir build ./src",
    "bundle": "browserify build/index.js --s LaTeX2HTML5 -o latex2html5.bundle.js"
  },
  "dependencies": {
    "d3": "^4.13.0",
    "latex2js": "^1.2.0",
    "latex2js-macros": "^1.1.5",
    "latex2js-mathjax": "^1.1.5",
    "latex2js-pstricks": "^1.2.0"
  },
  "devDependencies": {
    "browserify": "^16.1.1",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-react-app": "^3.1.1"
  }
}
