{
  "name": "@latex2js/vue",
  "version": "2.2.2",
  "description": "Vue components for LaTeX2JS",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/vue#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "keywords": [
    "latex",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "mathjax",
    "vue",
    "vue.js"
  ],
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist && copyfiles -u 1 src/latex.vue src/components/**/* dist/",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && pnpm run copy",
    "build:dev": "pnpm run clean && tsc --declarationMap && tsc -p tsconfig.esm.json && pnpm run copy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0",
    "vue-tsc": "^1.8.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "latex2js": "workspace:*",
    "@latex2js/macros": "workspace:*",
    "@latex2js/mathjax": "workspace:*",
    "@latex2js/pstricks": "workspace:*",
    "vue": "^3.3.0"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  }
}
