{
  "name": "@latex2js/html5",
  "version": "2.1.3",
  "description": "Author interactive math equations and diagrams online using LaTeX and PSTricks",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/html5#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && pnpm run copy",
    "build:dev": "pnpm run clean && tsc --declarationMap && tsc -p tsconfig.esm.json && pnpm run copy",
    "bundle": "browserify dist/index.js --s LaTeX2HTML5 -o latex2html5.bundle.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.0",
    "browserify": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "latex2js": "^2.1.2",
    "@latex2js/macros": "workspace:*",
    "@latex2js/mathjax": "workspace:*",
    "@latex2js/pstricks": "workspace:*"
  }
}
