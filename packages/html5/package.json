{
  "name": "@latex2js/html5",
  "version": "2.1.3",
  "description": "Author interactive math equations and diagrams online using LaTeX and PSTricks",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/html5#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "types": "main/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "scripts": {
    "build:main": "tsc --project tsconfig.build.json --outDir main",
    "build:module": "tsc --project tsconfig.build.json --outDir module --module esnext",
    "build": "pnpm run build:module && pnpm run build:main",
    "bundle": "browserify main/index.js --s LaTeX2HTML5 -o latex2html5.bundle.js",
    "prepare": "pnpm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.0",
    "browserify": "^16.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "latex2js": "^2.1.2",
    "@latex2js/macros": "workspace:*",
    "@latex2js/mathjax": "workspace:*",
    "@latex2js/pstricks": "workspace:*"
  }
}
