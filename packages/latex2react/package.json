{
  "name": "latex2react",
  "version": "2.1.3",
  "description": "Author interactive math equations and diagrams online using LaTeX and PSTricks",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/latex2react#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "main/index.js",
  "module": "module/index.js",
  "types": "main/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "keywords": [
    "latex",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "mathjax",
    "react"
  ],
  "scripts": {
    "build:main": "tsc --project tsconfig.build.json --outDir main",
    "build:module": "tsc --project tsconfig.build.json --outDir module --module esnext",
    "build": "pnpm run build:module && pnpm run build:main",
    "prepare": "pnpm run build",
    "lint:style": "stylelint 'src/**/*.js'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "regenerator-runtime": "^0.13.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "latex2js": "^2.1.2",
    "latex2js-macros": "^2.1.0",
    "latex2js-mathjax": "^2.1.0",
    "latex2js-pstricks": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
