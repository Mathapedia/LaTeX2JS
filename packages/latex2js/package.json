{
  "name": "latex2js",
  "version": "2.1.2",
  "description": "LaTeX and PSTricks rendering & components for the Web",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/latex2js#readme",
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "main": "main/index.js",
  "module": "module/index.js",
  "types": "main/index.d.ts",
  "directories": {
    "lib": "src",
    "test": "__tests__"
  },
  "files": [
    "main",
    "module",
    "latex2js.css",
    "latex2js.mathapedia.css"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "keywords": [
    "latex",
    "mathjax",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "pstricks",
    "graphics",
    "diagrams",
    "math diagrams"
  ],
  "scripts": {
    "build:main": "tsc --project tsconfig.build.json --outDir main",
    "build:module": "tsc --project tsconfig.build.json --outDir module --module esnext",
    "build": "pnpm run build:module && pnpm run build:main && pnpm run cp-styles",
    "cp-styles": "cp src/lib/*.css main/lib/ && cp src/lib/*.css module/lib/",
    "prepare": "pnpm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "dependencies": {
    "@latex2js/pstricks": "workspace:*",
    "@latex2js/settings": "workspace:*",
    "@latex2js/utils": "workspace:*"
  }
}
