{
  "name": "@latex2js/react",
  "version": "2.1.3",
  "description": "Author interactive math equations and diagrams online using LaTeX and PSTricks",
  "author": "Dan Lynch <pyramation@gmail.com>",
  "homepage": "https://github.com/pyramation/latex2js/tree/master/packages/react#readme",
  "license": "SEE LICENSE IN LICENSE",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pyramation/latex2js"
  },
  "keywords": [
    "latex",
    "latex2html",
    "latex2js",
    "math",
    "mathematics",
    "mathjax",
    "react"
  ],
  "scripts": {
    "copy": "copyfiles -f ../../LICENSE README.md package.json dist",
    "clean": "rimraf dist",
    "build": "pnpm run clean && tsc && tsc -p tsconfig.esm.json && pnpm run copy",
    "build:dev": "pnpm run clean && tsc --declarationMap && tsc -p tsconfig.esm.json && pnpm run copy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "type-check": "tsc --noEmit"
  },
  "bugs": {
    "url": "https://github.com/pyramation/latex2js/issues"
  },
  "devDependencies": {
    "@types/d3": "^7.4.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "regenerator-runtime": "^0.13.5",
    "stylelint": "^15.11.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.2.0",
    "copyfiles": "^2.4.1",
    "rimraf": "^5.0.5"
  },
  "dependencies": {
    "d3": "^7.8.0",
    "latex2js": "workspace:*",
    "@latex2js/macros": "workspace:*",
    "mathjaxjs": "workspace:*",
    "@latex2js/pstricks": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
